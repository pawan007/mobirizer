<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:padding="5dp" 
    android:background="#f26665"
    >
    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content" >
        <TextView
            android:id="@+id/tv_secondloco"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv_loco"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@+id/imageView2"
            android:paddingTop="15dp"
            android:text="secondlsrydfrtu" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_alignTop="@+id/tv_secondloco"
            android:layout_alignParentLeft="true"
            android:layout_marginTop="15dp"
            android:src="@drawable/locationiconforlist" />

        <TextView
            android:id="@+id/tv_loco"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_toRightOf="@+id/imageView1"
            android:layout_marginLeft="40dp"
            android:layout_marginTop="2dp"
            android:ellipsize="end"
            android:gravity="left"
            android:lines="1"
            android:maxLines="1"
            android:text="firstyudfrtg"
            android:textColor="#000000"
            android:textSize="15sp"
            android:textStyle="normal" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_below="@+id/imageView1"
            android:layout_toLeftOf="@+id/tv_secondloco"
            
            android:src="@drawable/messageiconforlist" />

        <TextView
            android:id="@+id/phoneno"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/tv_secondloco"
            android:layout_alignTop="@+id/imageView3"
            />
			<ImageView
            android:id="@+id/img_bulb"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginRight="15dp"
            />
			<ImageView
            android:id="@+id/img_bulbun"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginRight="15dp"
             />
    </RelativeLayout>

</FrameLayout>